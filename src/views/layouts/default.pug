include ../helpers/img
include ../helpers/head
include ../helpers/font
include ../helpers/style
include ../helpers/script
include ../helpers/global

doctype html
html(lang='en')
	head
		meta(charset='UTF-8')
		meta(http-equiv='X-UA-Compatible', content='IE=edge')
		meta(name='viewport', content='width=device-width, initial-scale=1.0')

		+head

		+style('style.css')

		+font('Roboto/Roboto-Regular.ttf')

		block head
			title Title

	body 
		block header

		block content
			main

		block footer

		+scriptVendor('jquery.min.js')
		//- @-auto-import

		+script('common.js')

		block script
